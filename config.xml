<?xml version='1.0' encoding='utf-8'?>
<widget id="ee.test" ios-CFBundleIdentifier="app.test.commons.love" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Commons OS</name>
    <description>
	Commons OS 
    </description>
    <author email="info@codeborne.com" href="http://www.codeborne.com">
       Codeborne 
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" target="app/google-services.json" />
    </platform>
    <platform name="ios">
        <resource-file src="GoogleService-Info.plist" />
        <config-file parent="UIStatusBarHidden" target="*-Info.plist">
            <true />
        </config-file>
        <config-file parent="UIRequiresFullScreen" target="*-Info.plist">
            <true />
        </config-file>
        <config-file parent="UIViewControllerBasedStatusBarAppearance" target="*-Info.plist">
            <false />
        </config-file>
        <config-file overwrite="true" parent="NSCameraUsageDescription" platform="ios" target="*-Info.plist">
            <string>Allow the app to use your camera</string>
        </config-file>
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/icon/ios/app-icon@180x180.png" width="180" />
        <icon height="60" src="res/icon/ios/app-icon@60x60.png" width="60" />
        <icon height="120" src="res/icon/ios/app-icon@120x120.png" width="120" />
        <icon height="76" src="res/icon/ios/app-icon@76x76.png" width="76" />
        <icon height="152" src="res/icon/ios/app-icon@152x152.png" width="152" />
        <icon height="40" src="res/icon/ios/app-icon@40x40.png" width="40" />
        <icon height="80" src="res/icon/ios/app-icon@80x80.png" width="80" />
        <icon height="72" src="res/icon/ios/app-icon@72x72.png" width="72" />
        <icon height="144" src="res/icon/ios/app-icon@144x144.png" width="144" />
        <icon height="167" src="res/icon/ios/app-icon@167x167.png" width="167" />
        <icon height="29" src="res/icon/ios/app-icon@29x29.png" width="29" />
        <icon height="58" src="res/icon/ios/app-icon@58x58.png" width="58" />
        <icon height="50" src="res/icon/ios/app-icon@50x50.png" width="50" />
        <icon height="100" src="res/icon/ios/app-icon@100x100.png" width="100" />
        <icon height="167" src="res/icon/ios/app-icon@167x167.png" width="167" />
        <icon height="1024" src="res/icon/ios/app-icon@1024x1024.png" width="1024" />
        <splash height="480" src="res/screen/ios/Default~iphone.png" width="320" />
        <splash height="960" src="res/screen/ios/Default~iphone@2x.png" width="640" />
        <splash height="1024" src="res/screen/ios/Default-Portrait~ipad.png" width="768" />
        <splash height="2048" src="res/screen/ios/Default-Portrait~ipad@2x.png" width="1536" />
        <splash height="768" src="res/screen/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="1536" src="res/screen/ios/Default-Landscape~ipad@2x.png" width="2048" />
        <splash height="1136" src="res/screen/ios/Default-568h@2x.png" width="640" />
        <splash height="1334" src="res/screen/ios/Default-667h.png" width="750" />
        <splash height="2208" src="res/screen/ios/Default-736h.png" width="1242" />
        <splash height="1242" src="res/screen/ios/Default-Landscape-736h.png" width="2208" />
        <splash height="2436" src="res/screen/ios/Default-812h.png" width="1125" />
        <splash height="2436" src="res/screen/ios/Default-Landscape-812h.png" width="1125" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="phonegap-plugin-push" spec="2.2.3">
        <variable name="ANDROID_SUPPORT_V13_VERSION" value="27.+" />
        <variable name="FCM_VERSION" value="11.6.2" />
    </plugin>
    <engine name="android" spec="^7.1.0" />
    <engine name="ios" spec="4.5.4" />
</widget>
