<?xml version='1.0' encoding='utf-8'?>
<widget id="ee.test" ios-CFBundleIdentifier="app.test.commons.love" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Commons OS</name>
    <description>
	Commons OS 
    </description>
    <author email="info@codeborne.com" href="http://www.codeborne.com">
       Codeborne 
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" target="app/google-services.json" />
    </platform>
    <platform name="ios">
        <resource-file src="GoogleService-Info.plist" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon src="res/ios/app-icon@180x180.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="res/ios/app-icon@60x60.png" width="60" height="60" />
        <icon src="res/ios/app-icon@120x120.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="res/ios/app-icon@76x76.png" width="76" height="76" />
        <icon src="res/ios/app-icon@152x152.png" width="152" height="152" />
        <!-- Spotlight Icon -->
        <icon src="res/ios/app-icon@40x40.png" width="40" height="40" />
        <icon src="res/ios/app-icon@80x80.png" width="80" height="80" />
        <!-- iPad -->
        <icon src="res/ios/app-icon@72x72.png" width="72" height="72" />
        <icon src="res/ios/app-icon@144x144.png" width="144" height="144" />
        <!-- iPad Pro -->
        <icon src="res/ios/app-icon@167x167.png" width="167" height="167" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="res/ios/app-icon@29x29.png" width="29" height="29" />
        <icon src="res/ios/app-icon@58x58.png" width="58" height="58" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="res/ios/app-icon@50x50.png" width="50" height="50" />
        <icon src="res/ios/app-icon@100x100.png" width="100" height="100" />
        <!-- iPad Pro -->
        <icon src="res/ios/app-icon@167x167.png" width="167" height="167" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="phonegap-plugin-push" spec="2.2.3">
        <variable name="ANDROID_SUPPORT_V13_VERSION" value="27.+" />
        <variable name="FCM_VERSION" value="11.6.2" />
    </plugin>
    <engine name="android" spec="^7.1.0" />
    <engine name="ios" spec="4.5.4" />
</widget>